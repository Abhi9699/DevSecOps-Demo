{
  "rest": {
    "RestGenerator": {
      "name": "Chatbot",
      "uri": "http://localhost:3000/api/chatbot",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "req_template_json_object": {
        "message": "$INPUT"
      },
      "response_json": true,
      "response_json_field": "reply"
    }
  }
}
